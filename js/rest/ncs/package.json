{
  "name": "ncs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "evomaster-client-js": "file:../evomaster-client-js",
    "jest": "^24.9.0",
    "babel-jest": "^24.9.0",
    "dedent": "^0.7.0",
    "superagent": "^5.1.1",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "check": "babel src -d check",
    "build": "babel  tests src -d build/src --copy-files",
    "test": "jest --coverage"
  },
  "babel": {
    "only": [
      "src"
    ],
    "plugins": [
      "module:evomaster-client-js"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*-test\\.(js|jsx|ts|tsx)$",
    "collectCoverageFrom": [
      "src/**/.*(js|jsx|ts|tsx)"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/",
      "<rootDir>/check/",
      "<rootDir>/build/"
    ]
  }
}
